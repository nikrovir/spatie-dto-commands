    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nikrovir\\SpatieDtoCommands\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nikrovir\\SpatieDtoCommands\\SpatieDTOCommandsProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "NikroVir",
            "email": "NikroVir@mail.ru"
        }
    ],
    "minimum-stability": "dev"
