    "keywords": [
        "spatie",
        "data-transfer-object",
        "spatie-dto-commands",
        "console",
        "nikrovir"
    ],
    "homepage": "https://github.com/nikrovir/spatie-dto-commands",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0",
